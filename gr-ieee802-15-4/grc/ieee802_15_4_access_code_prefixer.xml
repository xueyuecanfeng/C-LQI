<?xml version="1.0"?>

<block>
	<name>Access Code Prefixer</name>
	<key>ieee802_15_4_access_code_prefixer</key>
	<category>[IEEE802.15.4]</category>
	<import>import ieee802_15_4</import>
	<make>ieee802_15_4.access_code_prefixer($pad,$preamble)</make>

        <param>
		<name>Pad</name>
		<key>pad</key>
		<value>0x00</value>
		<type>hex</type>
	</param>
        <param>
		<name>Preamble</name>
		<key>preamble</key>
		<value>0x000000a7</value>
		<type>hex</type>
	</param>
	<sink>
		<name>in</name>
		<type>message</type>
	</sink>

	<source>
		<name>out</name>
		<type>message</type>
	</source>
</block>
