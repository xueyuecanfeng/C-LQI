<?xml version="1.0"?>
<block>
  <name>byteTo6bit</name>
  <key>byteTo6bit_byteTo6bit</key>
  <category>[byteTo6bit]</category>
  <import>import byteTo6bit</import>
  <make>byteTo6bit.byteTo6bit()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->


  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>48</vlen>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>byte</type>
    <vlen>288</vlen>
  </source>
</block>
